{
  "datasets": [
    {
      "name": "confusion_by_model_ds",
      "displayName": "Confusion Matrix by Model",
      "queryLines": [
        "SELECT \n",
        "CASE WHEN logs.model_name LIKE '%pytorch%' THEN 'PyTorch' WHEN logs.model_name LIKE '%classifier%' AND logs.model_name NOT LIKE '%pytorch%' THEN 'Keras' ELSE logs.model_name END as model_display_name, \n",
        "f.feedback_type, COUNT(*) as count FROM healthcare_catalog_dev.gold.prediction_feedback f \n",
        "INNER JOIN healthcare_catalog_dev.gold.pneumonia_predictions p ON f.prediction_id = p.prediction_id \n",
        "INNER JOIN \n",
        "      (\n",
        "            SELECT request_metadata['model_name'] as model_name, get_json_object(response, '$.predictions[0][0]') as pred_prob, timestamp_ms \n",
        "            FROM healthcare_catalog_dev.gold.pneumonia_classifier_payload WHERE status_code = 200\n",
        "      ) logs \n",
        "ON CAST(p.prediction_probability AS STRING) = CAST(logs.pred_prob AS STRING) AND ABS(unix_timestamp(p.predicted_at) * 1000 - logs.timestamp_ms) < 5000 \n",
        "\n",
        "GROUP BY CASE WHEN logs.model_name LIKE '%pytorch%' THEN 'PyTorch' WHEN logs.model_name LIKE '%classifier%' AND logs.model_name NOT LIKE '%pytorch%' THEN 'Keras' ELSE logs.model_name END, f.feedback_type \n",
        "ORDER BY \n",
        "CASE f.feedback_type WHEN 'true-positive' THEN 1 WHEN 'false-positive' THEN 2 WHEN 'false-negative' THEN 3 WHEN 'true-negative' THEN 4 ELSE 5 END"
      ]
    },
    {
      "name": "false_positives_ds",
      "displayName": "Recent False Positives",
      "queryLines": [
        "SELECT \n",
        "  f.feedback_id,\n",
        "  p.image_id,\n",
        "  p.predicted_at,\n",
        "  ROUND(p.prediction_probability, 3) as ai_confidence,\n",
        "  CASE WHEN logs.model_name LIKE '%pytorch%' THEN 'PyTorch' WHEN logs.model_name LIKE '%classifier%' AND logs.model_name NOT LIKE '%pytorch%' THEN 'Keras' ELSE logs.model_name END as model_name,\n",
        "  CONCAT('https://radiologist-feedback-review-3720263199172428.aws.databricksapps.com/?image=', p.image_id) as app_link\n",
        "FROM healthcare_catalog_dev.gold.prediction_feedback f\n",
        "INNER JOIN healthcare_catalog_dev.gold.pneumonia_predictions p ON f.prediction_id = p.prediction_id\n",
        "INNER JOIN (\n",
        "  SELECT request_metadata['model_name'] as model_name, get_json_object(response, '$.predictions[0][0]') as pred_prob, timestamp_ms \n",
        "  FROM healthcare_catalog_dev.gold.pneumonia_classifier_payload WHERE status_code = 200\n",
        ") logs ON CAST(p.prediction_probability AS STRING) = CAST(logs.pred_prob AS STRING) AND ABS(unix_timestamp(p.predicted_at) * 1000 - logs.timestamp_ms) < 5000\n",
        "WHERE f.feedback_type = 'false-positive'\n",
        "ORDER BY f.timestamp DESC\n",
        "LIMIT 10"
      ]
    },
    {
      "name": "error_by_split_ds",
      "displayName": "Errors by Dataset Split",
      "queryLines": [
        "SELECT \n",
        "  bronze.dataset_split,\n",
        "  f.feedback_type,\n",
        "  COUNT(*) as count\n",
        "FROM healthcare_catalog_dev.gold.prediction_feedback f\n",
        "INNER JOIN healthcare_catalog_dev.gold.pneumonia_predictions p ON f.prediction_id = p.prediction_id\n",
        "INNER JOIN healthcare_catalog_dev.bronze.kaggle_xray_metadata bronze ON p.image_id = bronze.image_id\n",
        "GROUP BY bronze.dataset_split, f.feedback_type\n",
        "ORDER BY bronze.dataset_split"
      ]
    },
    {
      "name": "size_accuracy_ds",
      "displayName": "File Size vs Accuracy",
      "queryLines": [
        "SELECT \n",
        "  CASE \n",
        "    WHEN bronze.file_size_bytes < 30000 THEN 'Small (<30KB)'\n",
        "    WHEN bronze.file_size_bytes < 60000 THEN 'Medium (30-60KB)'\n",
        "    ELSE 'Large (>60KB)'\n",
        "  END as size_category,\n",
        "  ROUND(SUM(CASE WHEN f.feedback_type IN ('true-positive', 'true-negative') THEN 1 ELSE 0 END) * 100.0 / COUNT(*), 2) as accuracy_pct,\n",
        "  COUNT(*) as total_predictions\n",
        "FROM healthcare_catalog_dev.gold.prediction_feedback f\n",
        "INNER JOIN healthcare_catalog_dev.gold.pneumonia_predictions p ON f.prediction_id = p.prediction_id\n",
        "INNER JOIN healthcare_catalog_dev.bronze.kaggle_xray_metadata bronze ON p.image_id = bronze.image_id\n",
        "GROUP BY CASE \n",
        "    WHEN bronze.file_size_bytes < 30000 THEN 'Small (<30KB)'\n",
        "    WHEN bronze.file_size_bytes < 60000 THEN 'Medium (30-60KB)'\n",
        "    ELSE 'Large (>60KB)'\n",
        "  END\n",
        "ORDER BY size_category"
      ]
    },
    {
      "name": "confidence_correctness_ds",
      "displayName": "Confidence vs Correctness",
      "queryLines": [
        "SELECT \n",
        "  ROUND(p.prediction_probability, 2) as confidence,\n",
        "  CASE WHEN f.feedback_type IN ('true-positive', 'true-negative') THEN 'Correct' ELSE 'Incorrect' END as correctness,\n",
        "  COUNT(*) as count\n",
        "FROM healthcare_catalog_dev.gold.prediction_feedback f\n",
        "INNER JOIN healthcare_catalog_dev.gold.pneumonia_predictions p ON f.prediction_id = p.prediction_id\n",
        "GROUP BY ROUND(p.prediction_probability, 2), CASE WHEN f.feedback_type IN ('true-positive', 'true-negative') THEN 'Correct' ELSE 'Incorrect' END"
      ]
    },
    {
      "name": "error_rate_time_ds",
      "displayName": "Error Rate Over Time",
      "queryLines": [
        "SELECT \n",
        "  DATE_TRUNC('day', f.timestamp) as date,\n",
        "  ROUND(SUM(CASE WHEN f.feedback_type IN ('false-positive', 'false-negative') THEN 1 ELSE 0 END) * 100.0 / COUNT(*), 2) as error_rate_pct,\n",
        "  COUNT(*) as total_predictions\n",
        "FROM healthcare_catalog_dev.gold.prediction_feedback f\n",
        "GROUP BY DATE_TRUNC('day', f.timestamp)\n",
        "ORDER BY date DESC"
      ]
    },
    {
      "name": "radiologist_agreement_ds",
      "displayName": "Per-Radiologist Agreement",
      "queryLines": [
        "SELECT \n",
        "  COALESCE(f.radiologist_id, 'Unknown') as radiologist_id,\n",
        "  ROUND(SUM(CASE WHEN f.feedback_type IN ('true-positive', 'true-negative') THEN 1 ELSE 0 END) * 100.0 / COUNT(*), 2) as agreement_pct,\n",
        "  COUNT(*) as total_reviews,\n",
        "  SUM(CASE WHEN f.feedback_type IN ('false-positive', 'false-negative') THEN 1 ELSE 0 END) as disagreements\n",
        "FROM healthcare_catalog_dev.gold.prediction_feedback f\n",
        "WHERE f.radiologist_id IS NOT NULL\n",
        "GROUP BY COALESCE(f.radiologist_id, 'Unknown')\n",
        "ORDER BY agreement_pct DESC"
      ]
    }
  ],
